const submitHandler = async (e: any) => {
  e.preventDefault();
  axios
    .put(`http://localhost:8080/ecom/api/auth/signup`, {
      email: email,
      username: username,
      password: password,
    })
    .then((response) => {
      console.log(response.data);
      router.push("/login");
    })
    .catch((error) => {
      console.log(error);
    });
};